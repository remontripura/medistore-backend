model Order {
    id          String      @id @default(uuid())
    userId      String
    orderCode   String?     @unique
    sellerId    String?
    address     String
    status      OrderStatus @default(PENDING)
    order_track OrderTrack  @default(ORDERD)
    quantity    Int
    medicineId  String?
    medicine    Medicine?   @relation(fields: [medicineId], references: [id])
    createdAt   DateTime    @default(now())
    updatedAt   DateTime    @updatedAt
}

enum OrderStatus {
    PENDING
    APPROVED
    REJECT
}

enum OrderTrack {
    ORDERD
    ORDERD_READY
    SHIPPED
    OUT_OF_DELIVERY
    DELIVERED
}
