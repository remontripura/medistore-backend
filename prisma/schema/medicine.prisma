model Medicine {
    id         String      @unique @default(uuid())
    images     String
    name       String
    price      String
    discount   String
    stock      Int         @default(0)
    categoriId String
    category   Categories  @relation(fields: [categoriId], references: [id], onDelete: Cascade)
    sellerId   String
    createdAt  DateTime    @default(now())
    updatedAt  DateTime    @updatedAt
    orders     Order[]
    orderItems OrderItem[]
}
